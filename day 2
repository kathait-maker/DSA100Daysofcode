#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    
    // Array with enough space (max size usually up to 10^5 in problems)
    int arr[100005];
    
    // Read n elements
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    int pos;
    scanf("%d", &pos);
    
    // Convert 1-based position to 0-based index
    int index = pos - 1;
    
    // Basic validation (optional but good practice)
    if(pos < 1 || pos > n) {
        // In most problems this case won't appear, but safe to handle
        printf("Invalid position\n");
        return 1;
    }
    
    // Shift elements to the left starting from index
    for(int i = index; i < n-1; i++) {
        arr[i] = arr[i+1];
    }
    
    // Now array has n-1 valid elements (last one is garbage)
    // Print first (n-1) elements
    for(int i = 0; i < n-1; i++) {
        if(i > 0) printf(" ");
        printf("%d", arr[i]);
    }
    printf("\n");
    
    return 0;
}
